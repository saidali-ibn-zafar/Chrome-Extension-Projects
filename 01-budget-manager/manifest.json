{
  "manifest_version": 2,
  "name": "Budget Manager",
  "version": "1.0",
  "description": "This extension tracks your overall spendings",
  "icons": {
    "128": "img.png",
    "48": "img.png",
    "16": "img.png"
  },
  "browser_action": {
    "default_icon": "img.png",
    "default_popup": "popup.html"
  },
  "permissions": ["storage"]
}
